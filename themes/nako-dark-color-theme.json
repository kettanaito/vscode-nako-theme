{
  "name": "vercel-theme",
  "type": "dark",
  "colors": {
    "selection.background": "#B731B0",
    "input.background": "#0D1217",
    "button.background": "#D74270",
    "button.foreground": "#fff",
    "quickInput.background": "#080808",
    "quickInputList.focusBackground": "#303030",
    "textLink.foreground": "#D74270",
    "textLink.activeForeground": "#fff",
    "focusBorder": "#D74270",

    /* Editor */
    "editor.background": "#080808",
    "editor.foreground": "#ddd",
    "activityBar.background": "#080808",
    "editorLineNumber.foreground": "#3f3f3f",
    "editorLineNumber.activeForeground": "#fff",
    "editor.selectionBackground": "#ffffff32",
    "editor.selectionForeground": "#fff",
    "editorCursor.foreground": "#B731B0",
    "editorWhitespace.foreground": "#2b2b2b",

    /* Sidebar */
    "sideBar.background": "#080808",
    "sideBar.border": "#2b2b2b",
    "sideBar.foreground": "#eee",
    "sideBarSectionHeader.background": "#080808",
    "sideBarSectionHeader.foreground": "#fff",
    "editorGroupHeader.border": "#303030",
    "sideBarSectionHeader.border": "#303030",
    "breadcrumb.foreground": "#aaa",

    /* Files list */
    "badge.background": "#D74270",
    "badge.foreground": "#fff",
    "list.activeSelectionBackground": "#D74270",
    "list.activeSelectionForeground": "#fff",
    "list.inactiveSelectionBackground": "#303030",
    "list.hoverBackground": "#303030",
    "debugIcon.breakpointForeground": "#D74270",

    /* Title bar */
    "titleBar.activeBackground": "#080808",
    "titleBar.activeForeground": "#fff",
    "titleBar.border": "#303030",

    /* Status bar */
    "statusBar.background": "#080808",
    "statusBar.foreground": "#6f6f6f",
    "statusBar.border": "#303030",
    "statusBarItem.warningBackground": "#E6913B",

    /* Activity bar */
    "activityBar.border": "#303030",
    "activityBarBadge.foreground": "#fff",
    "activityBarBadge.background": "#D74270",
    "activityBar.activeBorder": "#D74270",

    /* Tabs */
    "editorGroupHeader.tabsBackground": "#080808",
    "editorGroupHeader.tabsBorder": "#080808",
    "tab.border": "#303030",
    "tab.activeBackground": "#080808",
    "tab.activeForeground": "#fff",
    "tab.activeBorderTop": "#D74270",
    "tab.hoverBackground": "#30303075",
    "tab.inactiveBackground": "#080808",
    "tab.inactiveForeground": "#6f6f6f",
    "scrollbarSlider.background": "#303030",

    /* Terminal */
    "terminalCursor.foreground": "#B731B0",
    "terminal.tab.activeBorder": "#B731B0",

    /* Git */
    "gitDecoration.modifiedResourceForeground": "#E6913B",
    "gitDecoration.untrackedResourceForeground": "#B3D761",
    "gitDecoration.addedResourceForeground": "#B3D761",
    "gitDecoration.ignoredResourceForeground": "#6f6f6f"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6f6f6f"
      }
    },

    {
      "name": "Comments: Preprocessor",
      "scope": ["comment.block.preprocessor"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Comments: Documentation type bracets",
      "scope": [
        "comment.block.documentation punctuation.definition.bracket.curly"
      ],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Comments: Documentation Type",
      "scope": ["comment.block storage.type"],
      "settings": {}
    },
    {
      "name": "Comments: Documentation Class",
      "scope": ["comment.block.documentation punctuation.definition.block.tag"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": ["invalid.illegal"],
      "settings": {
        "foreground": "#cc0000"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Operators: TypeScript",
      "scope": ["keyword.operator.type"],
      "settings": {}
    },
    {
      "name": "Operators",
      "scope": ["meta.tag.attributes keyword.operator.assignment"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator.assignment"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator.arithmetic"],
      "settings": {}
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Keywords: Flow",
      "scope": [
        "keyword.control.flow",
        "keyword.control.default",
        "storage.modifier"
      ]
    },
    {
      "name": "Keyword Ini",
      "scope": ["keyword.other.definition.ini"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type", "keyword.control"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#2CB092"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language.import-export-all"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Variables",
      "scope": ["meta.object.member variable.other.constant"],
      "settings": {
        "foreground": "#36acaa"
      }
    },
    {
      "name": "Varialbes",
      "scope": ["meta.tag.attributes variable.other"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Variables: Read-write",
      "scope": ["meta.object.member variable.other.readwrite"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Functions",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Exceptions",
      "scope": ["entity.name.exception"],
      "settings": {
        "foreground": "#cc0000"
      }
    },
    {
      "name": "Sections",
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#D4D080"
      }
    },
    {
      "name": "Strings: Unquoted",
      "scope": ["string.unquoted"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": ["constant.other.symbol"],
      "settings": {
        "foreground": "#AB6526"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "punctuation.definition.quasi",
      "scope": ["punctuation.definition.quasi"],
      "settings": {
        "foreground": "#D4D080"
      }
    },
    {
      "name": "Punctuation: Keyword Definition",
      "scope": ["punctuation.definition.keyword"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#D4D080"
      }
    },
    {
      "name": "Punctuation: Entity Definition (Ini)",
      "scope": ["punctuation.definition.entity.ini"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Punctuation: Bold",
      "scope": ["punctuation.definition.bold"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Punctuation: Heading",
      "scope": ["punctuation.definition.heading"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation.definition.string.template"],
      "settings": {
        "foreground": "#2CB092"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["meta.tag.attributes punctuation.section.embedded"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Meta",
      "scope": [
        "meta.definition.variable",
        "meta.definition.variable entity.name.function"
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Meta: Braces",
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Meta: Method definition",
      "scope": [
        "meta.definition.method entity.name.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Meta: Inline Link",
      "scope": ["meta.link.inline"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    /* TypeScript */
    {
      "name": "Types",
      "scope": [
        "support.type.primitive",
        "meta.type.parameters entity.name.type"
      ],
      "settings": {
        "foreground": "#36ACAA"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#d9931e"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#d9931e"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "JSON: Property Names",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E6913B"
      }
    },
    {
      "name": "JSON: Punctuation",
      "scope": ["source.json punctuation.support.type.property-name"],
      "settings": {
        "foreground": "#E6913B"
      }
    },
    {
      "name": "CSS: Punctuation",
      "scope": ["punctuation.definition.entity.css"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#2CB092"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": ["keyword.other.important"],
      "settings": {
        "foreground": "#D74270"
      }
    },
    {
      "name": "Markup: Paragraph",
      "scope": ["meta.paragraph.markdown"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": [
        "markup.quote meta.paragraph.markdown",
        "markup.quote punctuation.definition"
      ],
      "settings": {
        "foreground": "#E6913B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": ["markup.bold.markdown"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": ["markup.error"],
      "settings": {
        "foreground": "#cc0000"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#ddd"
      }
    },
    {
      "name": "Markup: Link",
      "scope": ["meta.link"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": ["markup.prompt"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#AA3731"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": ["markup.traceback"],
      "settings": {
        "foreground": "#cc0000"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6f6f6f"
      }
    },
    {
      "name": "Markup Lists",
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#60A7FA"
      }
    },
    {
      "name": "Markup Styling",
      "scope": ["markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#2CB092"
      }
    },
    {
      "name": "Markup Inline",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#bd10e0"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": ["meta.diff.header.from-file"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": ["meta.diff.header.to-file"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "YAML",
      "scope": ["constant.other.timestamp"],
      "settings": {
        "foreground": "#B731B0"
      }
    },
    {
      "name": "YAML: Frontmatter strings",
      "scope": ["meta.embedded.block.frontmatter string.unquoted"],
      "settings": {
        "foreground": "#D4D080"
      }
    },
    /* PHP */
    {
      "name": "PHP: Begin block",
      "scope": ["punctuation.section.embedded.begin.php"],
      "settings": {
        "foreground": "#D74270"
      }
    }
  ]
}
